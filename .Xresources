URxvt.perl-ext-common: default,clipboard,matcher
URxvt.matcher.button: 1
URxvt.scrollstyle: plain
URxvt*font: xft:Inconsolata:style=Semibold:pixelsize=12:antialias=true:hinting=slight
URxvt*boldFont: xft:Inconsolata:style=bold:pixelsize=12:antialias=true:hinting=slight

URxvt*.transparent: true
URxvt*.shading: 110

! --- Xft settings
Xft.dpi:       96
Xft.antialias: 1
Xft.rgba:      rgb
Xft.autohint:  0
Xft.lcdfilter: lcddefault
Xft.hinting:   1
Xft.hintstyle: hintfull

